{
  // Use IntelliSense to find out which attributes exist for C# debugging
  // Use hover for the description of the existing attributes
  // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Subscriber",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-subscriber",
      "program": "${workspaceFolder}/Cbn.CleanSample.Subscriber/bin/Debug/netcoreapp2.1/Cbn.CleanSample.Subscriber.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Cbn.CleanSample.Subscriber",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "Cli",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-cli",
      "program": "${workspaceFolder}/Cbn.CleanSample.Cli/bin/Debug/netcoreapp2.1/Cbn.CleanSample.Cli.dll",
      "args": ["migration"],
      "cwd": "${workspaceFolder}/Cbn.CleanSample.Cli",
      "stopAtEntry": false,
      "console": "internalConsole"
    },
    {
      "name": "Web",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build-web",
      "program": "${workspaceFolder}/Cbn.CleanSample.Web/bin/Debug/netcoreapp2.1/Cbn.CleanSample.Web.dll",
      "args": [],
      "cwd": "${workspaceFolder}/Cbn.CleanSample.Web",
      "stopAtEntry": false,
      "internalConsoleOptions": "openOnSessionStart",
      "launchBrowser": {
        "enabled": true,
        "args": "${auto-detect-url}",
        "windows": {
          "command": "cmd.exe",
          "args": "/C start ${auto-detect-url}"
        },
        "osx": {
          "command": "open"
        },
        "linux": {
          "command": "xdg-open"
        }
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Cbn.CleanSample.Web/Views"
      }
    },
    {
      "name": ".NET Core Attach",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:pickProcess}"
    }
  ]
}
